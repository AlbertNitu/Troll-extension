{
  "manifest_version": 2,

  "name": "Adblock Plus",
  "description": "Hides any ads on a page",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "permissions": [
    "background"
  ],

  "background": {
    "scripts": ["backgroundimproved.js"]
  }

}